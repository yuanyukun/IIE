<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:background="@color/white"
    android:layout_height="80dp">
    <TextView
        android:layout_marginTop="15dp"
        android:id="@+id/profession_names"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/height_20dp"
        android:layout_centerVertical="true"
        android:text="11111"
        android:layout_marginBottom="15dp"
        android:textSize="16sp"/>
    <ImageView
        android:id="@+id/profession_check"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_centerVertical="true"
        android:layout_alignParentRight="true"
        android:layout_marginRight="@dimen/height_20dp"
        android:visibility="invisible"
        android:src="@mipmap/uncheck_48"/>
    <View
        style="@style/divider_line_horizontal"
        android:layout_alignParentBottom="true"/>
</RelativeLayout>