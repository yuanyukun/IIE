<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/grey_bg"
    tools:context="com.horem.parachute.mine.Mine_Account_Text_Modify">
    <EditText
        android:id="@+id/mine_account_edit_modify"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:layout_marginTop="@dimen/height_10dp"
        android:layout_marginLeft="@dimen/height_20dp"
        android:layout_marginRight="@dimen/height_20dp"
        android:padding="@dimen/height_10dp"
        android:gravity="left"
        android:background="@color/white"
        android:hint="自我描述..."
        android:textSize="14sp"
        android:textColor="@color/text_grey"
        android:paddingTop="@dimen/height_10dp"/>
</RelativeLayout>
